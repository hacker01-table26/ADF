				"name": "Copy_365",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "movies"
					},
					{
						"name": "Destination",
						"value": "challenge1/southridge/movies"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DocumentDbCollectionSource",
						"nestingSeparator": "."
